{
  "name": "md-to-html-static",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node build.mjs",
    "clean": "rimraf dist",
    "serve": "http-server dist -p 3000 -c-1",
    "watch:build": "chokidar \"content/**/*.md\" \"templates/**/*.html\" \"public/**/*\" -c \"npm run build\"",
    "dev": "npm-run-all --parallel watch:build serve"
  },
  "devDependencies": {
    "chokidar-cli": "^3.0.0",
    "fs-extra": "^11.3.2",
    "glob": "^10.4.5",
    "gray-matter": "^4.0.3",
    "html-escaper": "^3.0.3",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "rimraf": "^6.0.1",
    "unified": "^11.0.5"
  }
}
